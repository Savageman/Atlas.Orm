{
    "name": "atlas/orm",
    "type": "library",
    "description": "A data mapper ORM for your persistence model (not your domain model).",
    "keywords": [ "sql", "data", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Atlas",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Atlas/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "aura/sql": "~2.0",
        "aura/sqlquery": "~2.0",
        "capsule/di": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Orm\\": "src/"
        }
    },
    "require-dev": {
        "atlas/cli": "0.x-dev",
        "phpunit/phpunit": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\Orm\\": "tests/"
        }
    },
    "suggest": {
        "atlas/cli": "0.x command-line interface for atlas for development use"
    },
    "minimum-stability": "dev"
}
